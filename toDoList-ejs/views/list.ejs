<%- include("header") -%>

<div id="heading" class="box">
      <h1><%= listTitle %></h1>
    </div>
    
    <div class="box">
      <% for(let i=0; i<newListTask.length; i++) { %>
      <div class="item"> 
        <input type="checkbox" > 
        <p><%= newListTask[i] %></p>  
      </div>
      <% } %>

      <form class="item" action="/" method="post">
        <input type="text" name="newTask" placeholder="New Task" autocomplete="off"/>
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
      </form>
    </div>  
<%- include("footer") -%>
